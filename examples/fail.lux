[doc Demonstrate a failure]

[doc2 Provoke a failure to get something interesting in the logs]

[shell dc]
    # Multi-line send
    """!
    dc
    2
    3
    p
    +
    p
    """

    # Multi-line expect
    """?
    3
    5
    """

    # Demonstrate a failure
    [timeout 2]
    !1
    !+
    !p
    ?p
    ?7

[cleanup]
    !echo "LUX_START_REASON=$LUX_START_REASON"
